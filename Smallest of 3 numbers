/*Smallest of three numbers*/
#include<stdio.h>
int main()
{
	int a,b,c;
	printf("Enter a,b,c values\n");
	scanf("%d %d %d",&a,&b,&c);
	if(a>b)
	{
	
		if(b<c)
		printf("%d is the smallest number\n",b);
		else
		printf("%d is the smallest number\n",c);
}

	
	else
	{
		if(a<c)
		printf("%d is the smallest number",a);
		else
		printf("%d is the smallest number",c);
		
	}
	return 0;
}
